<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:WebWizard.Views"
                    xmlns:vms="clr-namespace:WebWizard.ViewModels">

    <DataTemplate DataType="{x:Type vms:WebTabViewModel}">
        <views:WebTabView Title="{Binding Title, Mode=OneWayToSource}" IsLoading="{Binding IsLoading, Mode=OneWayToSource}" Visibility="{Binding IsSelected, Converter={StaticResource BooleanToVisibilityConverter}}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vms:HomeTabViewModel}">
        <views:HomeTabView />
    </DataTemplate>
</ResourceDictionary>