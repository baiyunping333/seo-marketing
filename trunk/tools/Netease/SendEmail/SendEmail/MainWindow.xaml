<Window x:Class="SendEmail.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid ShowGridLines="True">
        <!--定义列 2列-->
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"></ColumnDefinition>
            <ColumnDefinition Width="2*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <!--定义行 1行-->
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Button Content="导入数据" Command="{Binding ImportAccountCommand}" Height="20" HorizontalAlignment="Left" Margin="1,3,0,0" Name="button1" VerticalAlignment="Top" Width="62" />
        <TabControl Height="286" HorizontalAlignment="Left" Margin="0,25,0,0" Name="tabControl1" VerticalAlignment="Top" Width="168">
            <TabItem Header="tabItem1" Name="tabItem1">
                <Grid>
                    <DataGrid ItemsSource="{Binding Accounts}" Margin="0,0,0,0" />
                </Grid>
            </TabItem>
            <TabItem Header="tabItem2" Name="tabItem2">
                
            </TabItem>
        </TabControl>

        <WebBrowser Height="220" Width="335" HorizontalAlignment="Left" Margin="0,0,0,0" Name="webB" VerticalAlignment="Top" Grid.Column="1" Initialized="webB_Initialized" />

    </Grid>
    
    
</Window>
